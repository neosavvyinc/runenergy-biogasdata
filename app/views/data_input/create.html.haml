.data-input.create{'ng-app' => 'dashboard'}
  = render 'shared/dashboard_actions', :landfill_operators => @landfill_operators, :sites => @sites, :monitor_classes => @monitor_classes, :sections => @sections
  %h5 Field Log
  .field-log.ng-clock{'ng-repeat' => 'flp in selectedMonitorClass.field_log_points'}
    %label{'ng-bind' => 'flp.name'}
    %input{'ng-model' => 'currentFieldLog[flp.name]'}
  %h5 Data